{
  "name": "<%= appname %>",
  "version": "1.0.0",
  "description": "Meshblu Connector <% classPrefix %>",
  "homepage": "<%= githubUrl %>/<%= appname %>",
  "bugs": "<%= githubUrl %>/<%= appname %>/issues",
  "author": {
    "name": "<%= realname %>",
    "url": "<%= githubUrl %>"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "<%= githubUrl %>/<%= appname %>.git"
